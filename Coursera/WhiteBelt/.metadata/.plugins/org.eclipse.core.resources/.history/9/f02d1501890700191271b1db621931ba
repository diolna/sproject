
#include <iostream>
#include <vector>
#include <sstream>
#include <functional>
#include <map>
using namespace std;

void Print(const map<int, int> m){
	for(auto& i: m){

			cout << "key = " << i.first << "value = "<< i.second << endl;
		}

}

int main() {
	int bbb = 2;
	map<int, int> m= { {1,2}, {2,56}, {45,6656}, {34, 4546}};
	auto i= m.begin();
	auto it = lower_bound(m.begin(), m.end(), [bbb](i){
		//return bbb == it->first;
	});

	cout << it->first<< endl;

	Print(m);
	m.erase(2);
	Print(m);


}
