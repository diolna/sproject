Date::Date() {}
//Date::Date(const vector<int> d) : date(d) {}

Date::Date(int y, int m, int d) :
	year(y), month(m), day(d) {}



int Date::GetYear()const  { return year; }
 int Date::GetMonth() const { return month; }
int Date::GetDay() const { return day; }
