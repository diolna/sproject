#pragma once
#include <vector>

using namespace std;


class Date {
public:
	Date(const int& year, const int& month, const int& day): year_(year), month_(month), day_(day){}
	Date(const vector<int> d) : date(d) {}
private:
	const int year_, month_, day_;
	vector<int> date = { year_, month_, day_ };

};
