#pragma once

#include <iostream>

using namespace std;

class Date {
public:
	Date(int year, int month, int day);
	const int GetYear() const ;
	const int GetMonth() const;
	const int GetDay() const ;

private:
	int year_;
	int month_;
	int day_;

};

bool operator<(const Date& lhs, const Date& rhs);
